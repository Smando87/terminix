{
	"name": "terminix",
	"description": "A VTE based terminal emulator for Linux",
	"copyright": "Copyright Â© 2015, Gerald Nunn",
	"authors": ["Gerald Nunn"],
    "dependencies": {
		"gtk-d": "3.3.0"
	},
	"buildTypes": {
		"release": {
            "versions": ["StdLoggerDisableTrace"]
		},
        "localize": {
            "versions": ["Localize"]
        }
	},
    "configurations": [
        {
            "name": "default",
            "targetType": "executable"    
        },
        {
            "name": "dynamic",
            "targetType": "executable",    
            "libs": ["gtkd-3"],
            "lflags": ["-defaultlib=gtkd-3.so"]
        }    
    ]    
}
